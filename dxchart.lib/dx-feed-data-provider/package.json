{
  "name": "@dx-private/dxchart-dx-feed-data-provider",
  "version": "0.6.0",
  "author": "devexperts",
  "license": "UNLICENSED",
  "scripts": {
    "watch": "tsc --watch --outDir dist",
    "build": "yarn clean_dist && yarn tsc",
    "build:production": "yarn clean_dist && yarn build && yarn obfuscate:lib && yarn add_disclaimer",
    "obfuscate:lib": "yarn ts-node --project ./tsconfig.node.json ../scripts/obfuscate-lib.ts",
    "add_disclaimer": "yarn ts-node --project ./tsconfig.node.json ../scripts/add-disclaimer.ts",
    "build:shadow_merge": "yarn test && yarn build",
    "clean_dist": "rimraf ./dist",
    "test": "jest --passWithNoTests && yarn test:lint",
    "test:lint": "eslint \"./lib/**/*.ts\" \"./lib/**/*.js\" --no-error-on-unmatched-pattern",
    "prepublishOnly": "yarn build:production"
  },
  "dependencies": {
    "@dxfeed/api": "^1.2.0",
    "@dxfeed/dxlink-websocket-client": "1.0.0-beta.4",
    "date-fns": "^2.30.0",
    "fp-ts": "^2.3.1",
    "fp-ts-rxjs": "^0.6.15",
    "rxjs": "^7.5.7"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.41.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "registry": "https://nexus.in.devexperts.com/repository/npm-dx-private/"
  },
  "files": [
    "dist"
  ]
}
